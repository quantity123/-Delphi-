unit Factory;

interface

uses
  LeiFeng;

type
  IFactory = interface
    function CreateLeiFeng: TLeiFeng;
  end;

  //学雷锋的大学生工厂
  TUndergraduateFactory = class(IFactory)
  public
    function CreateLeiFeng: TLeiFeng;
  end;

  //社区志愿者工厂
  TVolunteerFactory = class(IFactory)
  public
    function CreateLeiFeng: TLeiFeng;
  end;  

implementation

end.
